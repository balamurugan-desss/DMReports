@model Keyword_Suggesion.Models.Importfile

@{
    ViewBag.Title = "DM_ReportsImportFile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Import File</h2>

@using (Html.BeginForm("DM_ReportsImportFile", "Import", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.ImportType, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownListFor(model => model.ImportType, Keyword_Suggesion.Controllers.StaticValueController.ListImportType(), "--Select--", new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.ImportType, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="form-group">
            <div class="control-label col-md-2">
                <span>Select File  :</span>
            </div>
            <div class="col-md-10">
                <input class="form-control" type="file" name="postedFile" id="importFile" />
            </div>

        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input class="btn btn-primary" id="btnUpload" type="submit" value="Upload" />
            </div>
        </div>
        @*<div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Create" class="btn btn-default" />
                </div>
            </div>*@
    </div>
}

<div>
    @Html.ActionLink("Back to Project", "ProjectList", "Project_Keyword")
</div>

